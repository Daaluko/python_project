{% extends "base.jinja"%}

{% block content %}
<h1> Add a new city</h1>
{% for city in cities %}
    <div>
    {{ city.name }}
    {% endfor %}
    </div>
<form action="/cities" method="post">
    <label for="name">Name of city</label><br>
    <input type="text" id="name" name="name">
    <input type="submit" value="Add city">
</form>


{% endblock%}